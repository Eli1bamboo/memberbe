{
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true,
          "jsx": true
      },
      "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
      "indent": [1,2],
      "linebreak-style": [2,"unix"],
      "quotes": [1,"double"],
      "semi": [1,"never"]
  }
}
  